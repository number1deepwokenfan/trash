local amountnotifications = loadstring(game:HttpGet(("https://raw.githubusercontent.com/AbstractPoo/Main/main/Notifications.lua"),true))()


local Tax = game:GetService("Workspace").CountryData["Australia"].Economy.Tax.Value
local Upkeep = game:GetService("Workspace").CountryData["Australia"].Economy.Upkeep.Value
local TaxResource = game:GetService("Workspace").CountryData["Australia"].Economy.TaxResource.Value
local ResearchSpending = game:GetService("Workspace").CountryData["Australia"].Economy.ResearchSpending.Value
local StabilitySpending = game:GetService("Workspace").CountryData["Australia"].Economy.StabilitySpending.Value
local Reinforcement = game:GetService("Workspace").CountryData["Australia"].Economy.Reinforcement.Value
local BuildingExpenses = game:GetService("Workspace").CountryData["Australia"].Economy.BuildingExpenses.Value
--calculating electronics
local TotalBalance = (Tax + TaxResource - Upkeep - ResearchSpending - StabilitySpending - Reinforcement - BuildingExpenses)
local TotalBalance2 = (TotalBalance * 0.789)
local ElectronicsBal1 = (TotalBalance2/81600)

